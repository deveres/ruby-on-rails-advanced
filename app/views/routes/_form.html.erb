<%= form_with(model: route, local: true) do |form| %>

  <%=render 'common/errors' , resource: route %>



    <div class="field">
      <%= form.label :name %>
      <%= form.text_field :name, :class=>"form-control" %>
    </div>

    <div class="actions">
      <%= form.submit 'Сохранить',:class=>"btn btn-success" %>
    </div>
  <% end %>