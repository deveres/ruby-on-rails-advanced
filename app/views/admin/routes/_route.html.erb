<tr>
  <td><%= route.name %></td>
  <td><%= link_to t('common.view'), [:admin, route], :class => 'btn btn-xs btn-info' %></td>
  <td><%= link_to t('common.edit'), edit_admin_route_path(route), :class => 'btn btn-xs btn-success' %></td>
  <td><%= link_to t('common.delete'), [:admin, route], method: :delete, data: {confirm: t('common.confirm_delete')}, :class => 'btn btn-xs btn-danger' %></td>
</tr>